<%- include("partials/header"); -%>

<div id="homeBody">
  <h1>Home</h1>
  <p><%= startingContent %></p>

  <% posts.forEach(function(post){ %>

  <h1><%=post.title%></h1>
  <p>
    <%=post.content.substring(0, 100) + " ..."%>
    <a href="/posts/<%=post._id%>">Read More</a>
  </p>

  <% }) %>
</div>
<%- include("partials/footer"); -%>
